{
  "compilerOptions": {
    "target": "es2017",
    "outDir": "./dist",
    "strict": true,
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "declaration": true,
    "emitDeclarationOnly": true,
    "allowImportingTsExtensions": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"],
  "typeRoots": ["./node_modules/@types", "./node_modules/@discordjs/typings"]
}
